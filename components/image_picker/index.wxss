.image_wrap {
  width: 100%;
  height: 180rpx;
  margin-left: 10rpx;
}
.image_wrap .image_scroll {
  display: flex;
  margin-top: 20rpx;
}
.image_wrap .image_scroll .image_item {
  position: relative;
  margin-right: 20rpx;
}
.image_wrap .image_scroll .image_item image {
  width: 150rpx;
  height: 150rpx;
}
.image_wrap .image_scroll .image_item icon {
  position: absolute;
  right: -7rpx;
  top: -7rpx;
}
.image_wrap .image_scroll image {
  width: 150rpx;
  height: 150rpx;
}
