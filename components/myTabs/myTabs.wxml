<view class="tabs">
    <view class="tabs_title">
        <view class="title {{activeTab === index ? 'active' : ''}}" wx:for="{{tabs}}" wx:key="id" bindtap="handleItemClick" data-index="{{index}}">
            {{item.title}}
        </view>
    </view>
    <view class="tabs_content">
        <slot></slot>
    </view>
</view>