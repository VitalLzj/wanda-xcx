<import src="../pop_header/index.wxml" />
<import src="../pop_footer/index.wxml" />
<template name="bxlx">
    <view class="tmp_pop">
        <template is="pop_header" data="{{title}}" />
        <view class="tmp_content">
            <van-cell bind:tap="handleBxlxItemClick" data-index="{{index}}" wx:for="{{bxlx_arr}}" title="{{item.description}}" wx:key="value" />
        </view>
        <template is="pop_footer" />
    </view>
</template>